.info{
  background-color: #fff;
  margin-bottom: 30rpx;
}

.cell {
  font-size: 34rpx;
  padding: 28rpx 28rpx 28rpx 0;
  margin-left: 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.cell + .cell{
  border-top: 1px solid #f5f5f5;
}

.tag{
  display: inline-block;
  width: 240rpx;
}

