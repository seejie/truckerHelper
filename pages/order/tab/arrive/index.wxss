@import "../../../../app.wxss";

.info{
  background-color: #fff;
}

.cell {
  font-size: 34rpx;
  padding: 28rpx 28rpx 28rpx 0;
  margin-left: 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.cell + .cell{
  border-top: 1px solid #f5f5f5;
}

.tag{
  display: inline-block;
  width: 240rpx;
}

.arrive-wrapper{
  display: flex;
  flex-direction: column;
  /* height: calc(100vh - 246rpx); */
  height: 100%;
}

/* 提示信息 */
.tips{
  color: #999;
  /* margin: 45rpx 30rpx; */
  margin: 15rpx 30rpx;
  align-items: start;
}

.tips-icon{
  flex-shrink: 0;
  margin-top: 8rpx;
}

.container{
  flex: 1;
}

.map{
  height: 100%;
}
