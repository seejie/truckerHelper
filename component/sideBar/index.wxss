.wrapper{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
}

.icon-bar{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #7F8389;
  font-size: 20rpx;
}

.icon-bar.active{
  color: #000099;
}

.img{
  width: 54rpx;
  height: 54rpx;
}
